    "extra": {
        "name": "Migrate From WordPress",
        "handle": "migrate-from-wordpress",
        "developer": "vnali",
        "developerUrl": "https://github.com/vnali",
        "documentationUrl": "https://github.com/vnali/migrate-from-wordpress-plugin-docs",
        "changelogUrl": "https://raw.githubusercontent.com/vnali/migrate-from-wordpress/v0/CHANGELOG.md",
        "class": "vnali\\migratefromwordpress\\MigrateFromWordPress"
