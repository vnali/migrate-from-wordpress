{
    "name": "vnali/migrate-from-wordpress",
    "description": "Migrate From WordPress to Craft CMS",
    "type": "craft-plugin",
    "version": "0.4.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "wordpress"
    ],
    "license": "MIT",
    "authors": [{
        "name": "vnali",
        "homepage": "https://github.com/vnali"
    }],
    "support": {
        "docs": "https://github.com/vnali/migrate-from-wordpress-plugin-docs",
        "issues": "https://github.com/vnali/migrate-from-wordpress/issues"
    },
    "require": {
        "craftcms/cms": "^4.0.0",
        "craftcms/feed-me": "^5.0.0",
        "symfony/dom-crawler": "^6.0.0",
        "symfony/css-selector": "^6.0.0"    
    },
    "autoload": {
        "psr-4": {
            "vnali\\migratefromwordpress\\": "src/"
        }
